{
  "env": {
    "browser": true,
    "webextensions": true,
    "es6": true,
  },
  "rules": {
    "strict": [2, "global"],
    "max-len": [2, {
      "code": 100
    }],
    "no-console": [0],
    "no-underscore-dangle": [0],
    "func-names": [0],
    "no-use-before-define": [0],
    "no-param-reassign": [0]
  },
  "extends": "airbnb-base"
}
